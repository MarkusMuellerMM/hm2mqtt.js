    "mqtt": "^2.11.0",
    "persist-json": "^1.1.0",
    "request": "^2.81.0",
    "yalm": "^4.0.2",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "camo-purge": "latest",
